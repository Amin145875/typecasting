#include<iostream>
using namespace std;

int main()
{
    //devclare a variable

    int num1;
    int num2;
    int sum;

    cout<<"enter the first number!\n";
    cin>>num1;
    cout<<"enter another number!\n";
    cin>>num2;
    sum=num1+num2;
    cout<<"the sum of those numbers"<<sum<<endl;


    return 0;
}
